 {
     quintptr stackLimit;
-#if USE(PTHREADS) && !OS(QNX)
+#if defined(Q_OS_GENODE)
+	Genode::Thread::Stack_info stack_info = Genode::Thread::mystack();
+	stackLimit = reinterpret_cast<quintptr>(reinterpret_cast<void*>(stack_info.base));
+#elif USE(PTHREADS) && !OS(QNX)

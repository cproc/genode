<config>
	<parent-provides>
		<service name="CPU"/>
		<service name="File_system"/>
		<service name="LOG"/>
		<service name="Nitpicker"/>
		<service name="PD"/>
		<service name="Report"/>
		<service name="ROM"/>
		<service name="Timer"/>
	</parent-provides>

	<default-route> <any-service> <parent/> <any-child/> </any-service> </default-route>

	<default caps="100"/>

	<start name="textedit" caps="300">
		<resource name="RAM" quantum="60M"/>
		<config>
			<vfs>
				<dir name="dev">
					<log/>
					<inline name="rtc">2018-01-01 00:01</inline>
				</dir>
				<dir name="edit"> <fs label="rw"/> </dir>
				<tar name="qt5_dejavusans.tar"/>
			</vfs>
			<libc stdout="/dev/log" stderr="/dev/log" rtc="/dev/rtc"/>
		</config>
		<route>
			<service name="File_system" label="rw"> <parent label="rw"/> </service>
			<service name="Nitpicker"> <parent label=""/> </service>
			<service name="Report" label="shape"> <parent label="shape"/> </service>
			<any-service> <parent /> </any-service>
		</route>
	</start>
</config>

<config verbose="yes" prio_levels="2">

	<report init_ram="yes" requested="yes" child_ram="yes" delay_ms="2000" buffer="128K"/>

	<parent-provides>
		<service name="ROM"/>
		<service name="PD"/>
		<service name="RM"/>
		<service name="CPU"/>
		<service name="LOG"/>
		<service name="Nitpicker"/>
		<service name="Timer"/>
		<service name="Block"/>
		<service name="Rtc"/>
		<service name="Report"/>
		<service name="File_system"/>
		<service name="Usb"/>
		<service name="Platform"/>
	</parent-provides>

	<default-route> <any-service> <parent/> <any-child/> </any-service> </default-route>

	<default caps="100"/>

	<start name="fs" caps="500">
		<resource name="RAM" quantum="104M"/>
		<binary name="init"/>
		<route>
			<service name="ROM" label="config">
				<parent label="config -> subinit/default_fs.config"/> </service>
			<any-service> <parent/> </any-service>
		</route>
		<provides> <service name="File_system"/> </provides>
	</start>

	<start name="noux" caps="1000">
		<resource name="RAM" quantum="80M"/>
		<binary name="init"/>
		<route>
			<service name="ROM" label="config">
				<parent label="config -> subinit/default_noux.config"/> </service>
			<service name="File_system" label="config">
				<parent label="config"/> </service>
			<service name="File_system" label="target">
				<child name="fs"/> </service>
			<any-service> <parent/> </any-service>
		</route>
	</start>

	<start name="nic" caps="500">
		<resource name="RAM" quantum="36M"/>
		<binary name="init"/>
		<route>
			<service name="ROM" label="config">
				<parent label="config -> subinit/default_nic.config"/> </service>
			<service name="ROM" label="wlan.config">
				<parent label="config -> wlan.config"/> </service>
			<service name="LOG"> <parent label="unlogged"/> </service>
			<any-service> <parent/> </any-service>
		</route>
		<provides> <service name="Nic"/> </provides>
	</start>

	<start name="timer">
		<resource name="RAM" quantum="1M"/>
		<provides><service name="Timer"/></provides>
	</start>

	<start name="usb_devices_rom">
		<binary name="rom_filter" />
		<resource name="RAM" quantum="2M"/>
		<provides> <service name="ROM"/> </provides>
		<config>
			<output node="devices">
				<inline>
				</inline>
			</output>
		</config>
	</start>

	<start name="vbox_nit_fb" priority="0">
		<binary name="nit_fb"/>
		<resource name="RAM" quantum="8M" />
		<provides>
			<service name="Framebuffer" /> <service name="Input" />
		</provides>
		<config xpos="150" ypos="30" width="-160" height="-40" />
	</start>

	<start name="vbox" priority="-1" caps="1000">
		<binary name="virtualbox5-nova" />
		<resource name="RAM" quantum="8G"/>
			<config vbox_file="machine.vbox" xhci="yes" vm_name="linux">
			<vfs>
				<dir name="dev">
					<log/>
					<rtc/>
				 </dir>
				<dir name="shared"> <fs label="shared"/> </dir>
				<fs root="/vm/sculpt" writeable="yes"/>
			</vfs>
			<libc stdout="/dev/log" stderr="/dev/log" rtc="/dev/rtc"/>
		</config>
		<route>
			<service name="File_system"> <child name="fs"/> </service>
			<service name="ROM" label="usb_devices"> <child name="usb_devices_rom"/> </service>
			<service name="Nic"> <child name="nic"/> </service>
			<any-service> <parent/> <child name="vbox_nit_fb"/> </any-service>
		</route>
	</start>

</config>


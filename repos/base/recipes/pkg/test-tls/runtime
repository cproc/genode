<runtime ram="32M" caps="1000" binary="init">

	<events>
		<timeout meaning="failed" sec="20" />
		<log meaning="succeeded">
			[init -> test-tls] thread 0: x: 0, y: 1
			[init -> test-tls] thread 1: x: 2, y: 3
			[init -> test-tls] main:     x: 4, y: 5
		</log>
	</events>

	<content>
		<rom label="ld.lib.so"/>
		<rom label="test-tls"/>
	</content>

	<config>
		<parent-provides>
			<service name="LOG"/>
			<service name="PD"/>
			<service name="CPU"/>
			<service name="ROM"/>
		</parent-provides>
		<default-route>
			<any-service> <parent/> </any-service>
		</default-route>
		<default caps="50"/>
		<start name="test-tls">
			<resource name="RAM" quantum="10M"/>
		</start>
	</config>
</runtime>
